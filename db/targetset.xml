<?xml version="1.0" ?>
<gprconfig>
  <!-- x86 linux -->
  <targetset>
    <target>i686-pc-linux-gnu</target>
    <target>i586-suse-linux</target>
    <target>i386-redhat-linux</target>
  </targetset>

  <!-- ia64 linux -->
  <targetset>
    <target>ia64-unknown-linux-gnu</target>
    <target>ia64-sgi-linux-gnu</target>
    <target>ia64-hp-linux-gnu</target>
    <target>ia64-redhat-linux</target>
    <target>ia64-suse-linux</target>
  </targetset>

  <!-- x86_64 linux -->
  <targetset>
    <target>x86_64-pc-linux-gnu</target>
    <target>x86_64-unknown-linux-gnu</target>
    <target>x86_64-redhat-linux</target>
    <target>x86_64-suse-linux</target>
  </targetset>

  <!-- x86 solaris -->
  <targetset>
    <target>i386-pc-solaris2.10</target>
    <target>i686-pc-solaris2.10</target>
    <target>i686-pc-solaris2.8</target>
    <target>i386-pc-solaris2.8</target>
  </targetset>

  <!-- x86 windows mingw-->
  <targetset>
    <!--  known targets: pentium-mingw32msv, i686-pc-mingw32 -->
    <target>i.*-mingw32.*</target>
    <target>pent.*-mingw32.*</target>
  </targetset>

    <!-- x86 windows cygwin -->
  <targetset>
    <!--  known targets: pentium-mingw32msv, i686-pc-cygwin -->
    <target>i.*-cygwin.*</target>
    <target>pent.*-cygwin.*</target>
  </targetset>

</gprconfig>
