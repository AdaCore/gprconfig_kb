   <!ENTITY environment
    '
   <configuration>
    &if_this_target;

    <config>

   EXEC_ADA := "${EXEC(Ada)}";
   EXEC_C   := "${EXEC(C)}";
   EXEC_CPP := "${EXEC(C++)}";

   HOST_ADA := "${HOST(Ada)}";
   HOST_C   := "${HOST(C)}";
   HOST_CPP := "${HOST(C++)}";

   TARGET_ADA := "${TARGET(Ada)}";
   TARGET_C   := "${TARGET(C)}";
   TARGET_CPP := "${TARGET(C++)}";

   VERSION_ADA := "${VERSION(Ada)}";
   VERSION_C   := "${VERSION(C)}";
   VERSION_CPP := "${VERSION(C++)}";

   PREFIX_ADA := "${PREFIX(Ada)}";
   PREFIX_C   := "${PREFIX(C)}";
   PREFIX_CPP := "${PREFIX(C++)}";

   PATH_ADA := "${PATH(Ada)}";
   PATH_C   := "${PATH(C)}";
   PATH_CPP := "${PATH(C++)}";

   RUNTIME_ADA := "${RUNTIME(Ada)}";
   RUNTIME_C   := "${RUNTIME(C)}";
   RUNTIME_CPP := "${RUNTIME(C++)}";

   GPRCONFIG_PREFIX := "${GPRCONFIG_PREFIX}";

    </config>
   </configuration>
    '
   >

